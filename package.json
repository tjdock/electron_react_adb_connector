{
  "name": "electron_adb_template",
  "version": "1.0.0",
  "author": {
    "name": "tjivy",
    "email": "xunuo@tjivy.com"
  },
  "homepage": "./",
  "description": "xxx管理系统",
  "main": "public/electron.js",
  "repository": "https://github.com/tjdock/cnooc",
  "private": true,
  "dependencies": {
    "adbkit": "^2.11.1",
    "electron-is-dev": "^1.2.0",
    "form-data": "^3.0.0",
    "sqlite3": "^5.0.1",
    "tedious": "^9.2.3",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "electron": "^11.2.0",
    "electron-builder": "^22.9.1",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-highlight-words": "^0.16.1",
    "@types/react-redux": "^7.1.15",
    "@types/react-router-dom": "^5.1.7",
    "antd": "^4.10.2",
    "connected-react-router": "^6.8.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "nprogress": "^0.2.0",
    "cross-env": "^7.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-highlight-words": "^0.16.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "tsc": "tsc src/electron/electron.ts --outDir public/",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "onlyEle": "npm run tsc && chcp 65001 && nodemon --watch src/electron -e ts --exec \"electron .\"",
    "prerelease": "npm run build",
    "release": "electron-builder"
  },
  "license": "ISC",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "cn.com.cnooc.well_control_inspection",
    "productName": "${description}",
    "copyright": "Copyright © 2020 ${author}",
    "publish": null,
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraFiles": [],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/icon.ico",
      "artifactName": "${productName}-${version}.${ext}",
      "publisherName": "tjivy"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "perMachine": false
    }
  }
}